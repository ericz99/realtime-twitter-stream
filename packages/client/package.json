{
  "name": "static-webpack-boilerplate",
  "version": "1.0.0",
  "description": "This is mainly for static project or any frontend projects, this boilerplate allows you to minifiy your source code into your build folder!",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js",
    "start": "webpack --config webpack/webpack.config.dev.js --mode=development --watch",
    "dev": "webpack-dev-server --config webpack/webpack.config.dev.js --mode=development --hot",
    "server": "node index.js"
  },
  "repository": "https://github.com/ericz99/static-webpack-boilerplate.git",
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "Eric Zhang <ericzhang128@gmail.com>",
  "license": "MIT",
